	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a2e2be88"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.comment",
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.definition.annotation",
				"punctuation.definition.markdown"
			],
			"settings": {
				"foreground": "#a2e2be88"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"punctuation.section",
				"variable",
				"constant.character",
				"constant.other",
				"support.variable.property.dom.js",
				"meta.method.declaration.js"
			],
			"settings": {
				// "foreground": "#dbddc5"
				"foreground": "#fafbdc"
			}
		},
		{
			"name": "#8ff0ac",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.numeric",
				"variable.language"
			],
			"settings": {
				"foreground": "#aef8c4"
			}
		},
		{
			"name": "Regular Expressions and Escape Characters",
			"scope": [
				"string.regexp",
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#95e6cb"
			}
		},
		{
			"name": "#f27c74",
			"scope": [
				"variable.member",
				"keyword",
				"keyword.operator",
				"punctuation.accessor",
				"punctuation.definition.tag.end",
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag",
				"support.variable.dom.js",
				"storage",
				"storage.type",
				"support.constant",
				"support.function",
				"support.macro",
				"constant.numeric.line-number.match",
				"punctuation.definition.image",
				"entity.name",
				"entity.other.inherited-class",
				"source.css entity.name.tag",
				"source.sass entity.name.tag",
				"source.scss entity.name.tag",
				"source.less entity.name.tag",
				"source.stylus entity.name.tag",
				"entity.name.tag",
				"meta.tag.sgml",
				"support.type",
				"support.class",
				"support.type.property-name",
			],
			"settings": {
				"foreground": "#f27c74"
			}
		},
		{
			"name": "Function name",
			"scope": [
				
				"variable.function",
				"variable.annotation",
				"meta.function-call.generic",
				"source.ruby variable.other.readwrite",
				"entity.name.import",
				"entity.name.package",
